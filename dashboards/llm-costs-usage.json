    {
      "id": 2,
      "title": "Cost per Request (avg)",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "datasource": {
        "type": "postgres",
        "uid": "litellm-postgres"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 4,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.1 }
            ]
          },
          "color": {
            "mode": "thresholds"
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "wideLayout": true,
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "litellm-postgres"
          },
          "rawSql": "SELECT\n  ROUND(SUM(spend)::numeric / NULLIF(SUM(api_requests), 0), 4) AS \"Avg Cost per Request ($)\"\nFROM \"LiteLLM_DailyUserSpend\"\nWHERE $__timeFilter(date)",
          "format": "table",
          "refId": "A",
          "editorMode": "code"
        }
      ]
    },
    {
