        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "wideLayout": true,
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "litellm-postgres"
          },
          "rawSql": "SELECT\n  ROUND(\n    COUNT(CASE WHEN cache_hit = 'True' OR cache_hit = 'true' THEN 1 END)::numeric /\n    NULLIF(COUNT(*)::numeric, 0) * 100,\n    1\n  ) AS \"Cache Hit Rate %\"\nFROM \"LiteLLM_SpendLogs\"\nWHERE $__timeFilter(\"startTime\")",
          "format": "table",
          "refId": "A",
          "editorMode": "code"
        }
      ]
