      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "litellm-postgres"
          },
          "rawSql": "SELECT\n  date_trunc('hour', \"startTime\") AS time,\n  PERCENTILE_CONT(0.5) WITHIN GROUP (ORDER BY EXTRACT(EPOCH FROM (\"endTime\" - \"startTime\"))) AS p50_seconds,\n  PERCENTILE_CONT(0.95) WITHIN GROUP (ORDER BY EXTRACT(EPOCH FROM (\"endTime\" - \"startTime\"))) AS p95_seconds\nFROM \"LiteLLM_SpendLogs\"\nWHERE $__timeFilter(\"startTime\")\nGROUP BY 1\nORDER BY 1",
