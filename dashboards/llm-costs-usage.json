    {
      "id": 11,
      "title": "Week-over-Week Cost Change",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 36
      },
      "datasource": {
        "type": "postgres",
        "uid": "litellm-postgres"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "orange", "value": 25 },
              { "color": "red", "value": 50 }
            ]
          },
          "color": { "mode": "thresholds" }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "litellm-postgres" },
          "rawSql": "WITH this_week AS (\n  SELECT COALESCE(SUM(spend), 0) AS spend\n  FROM \"LiteLLM_DailyUserSpend\"\n  WHERE date >= date_trunc('week', CURRENT_DATE)\n),\nlast_week AS (\n  SELECT COALESCE(SUM(spend), 0) AS spend\n  FROM \"LiteLLM_DailyUserSpend\"\n  WHERE date >= date_trunc('week', CURRENT_DATE) - INTERVAL '7 days'\n    AND date < date_trunc('week', CURRENT_DATE)\n)\nSELECT\n  ROUND(\n    (this_week.spend - last_week.spend)::numeric /\n    NULLIF(last_week.spend, 0) * 100,\n    1\n  ) AS \"WoW Cost Change %\"\nFROM this_week, last_week",
          "format": "table",
          "refId": "A",
          "editorMode": "code"
        }
      ]
    }
