    {
      "id": 12,
      "title": "New vs Returning Users",
      "type": "timeseries",
      "datasource": {
        "type": "mysql",
        "uid": "workoflow-mariadb"
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 38
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0,
            "barWidthFactor": 0.8,
            "fillOpacity": 80,
            "lineWidth": 1,
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "mode": "normal", "group": "A" },
            "axisPlacement": "auto",
            "axisBorderShow": false
          },
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "New Users" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Returning Users" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "blue" } }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["sum"]
        }
      },
      "targets": [
        {
          "datasource": { "type": "mysql", "uid": "workoflow-mariadb" },
          "rawSql": "SELECT\n  d.day AS time,\n  COUNT(DISTINCT CASE WHEN DATE(u.created_at) = d.day THEN al.user_id END) AS \"New Users\",\n  COUNT(DISTINCT CASE WHEN DATE(u.created_at) < d.day THEN al.user_id END) AS \"Returning Users\"\nFROM (\n  SELECT DISTINCT DATE(created_at) AS day\n  FROM audit_log\n  WHERE $__timeFilter(created_at)\n) d\nJOIN audit_log al ON DATE(al.created_at) = d.day\nJOIN user u ON al.user_id = u.id\nWHERE $__timeFilter(al.created_at)\nGROUP BY d.day\nORDER BY d.day",
          "refId": "A",
          "format": "table"
        }
      ]
    }
