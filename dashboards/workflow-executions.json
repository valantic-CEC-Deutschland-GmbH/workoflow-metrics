          "rawSql": "SELECT\n  ROUND(AVG(EXTRACT(EPOCH FROM (\"stoppedAt\" - \"startedAt\")))::numeric, 1) AS \"Avg Duration (s)\"\nFROM execution_entity\nWHERE $__timeFilter(\"startedAt\")\n  AND \"stoppedAt\" IS NOT NULL",
