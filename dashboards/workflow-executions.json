          "datasource": {
            "type": "postgres",
            "uid": "n8n-internal"
          },
          "rawSql": "SELECT\n  w.name AS \"Workflow\",\n  SUM(CASE WHEN e.status = 'error' THEN 1 ELSE 0 END) AS \"Failures\",\n  ROUND(\n    SUM(CASE WHEN e.status = 'error' THEN 1 ELSE 0 END)::numeric /\n    NULLIF(COUNT(*)::numeric, 0) * 100,\n    1\n  ) AS \"Failure Rate %\"\nFROM execution_entity e\nJOIN workflow_entity w ON e.\"workflowId\" = w.id\nWHERE $__timeFilter(e.\"startedAt\")\nGROUP BY w.name\nHAVING SUM(CASE WHEN e.status = 'error' THEN 1 ELSE 0 END) > 0\nORDER BY \"Failures\" DESC\nLIMIT 10",
