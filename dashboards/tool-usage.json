              "viz": false,
              "legend": false
            },
            "barAlignment": 0,
            "stacking": {
              "mode": "none",
              "group": "A"
            }
          },
          "color": {
            "mode": "palette-classic"
          },
          "mappings": [],
          "thresholds": {
