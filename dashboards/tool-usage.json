        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": false,
          "calcs": []
        },
        "xTickLabelRotation": 0,
        "showValue": "auto",
        "barWidth": 0.8,
        "groupWidth": 0.7,
        "stacking": "none"
      },
      "pluginVersion": "10.4.0",
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "mysql",
            "uid": "workoflow-mariadb"
          },
          "rawSql": "SELECT\n  JSON_UNQUOTE(JSON_EXTRACT(data, '$.tool_name')) AS tool_name,\n  COUNT(*) AS usage_count\nFROM audit_log\nWHERE action LIKE 'tool_execution%'\n  AND $__timeFilter(created_at)\nGROUP BY tool_name\nORDER BY usage_count DESC\nLIMIT 15",
          "format": "table",
          "editorMode": "code"
        }
      ]
    },
    {
      "id": 5,
      "title": "Tool Success/Fail Breakdown",
      "type": "piechart",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "datasource": {
        "type": "mysql",
        "uid": "workoflow-mariadb"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "mappings": [],
