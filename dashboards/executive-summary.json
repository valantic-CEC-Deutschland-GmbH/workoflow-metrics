      "tags": [],
      "targetBlank": false,
      "title": "Workflow Executions",
      "tooltip": "Detailed workflow execution metrics",
      "type": "link",
      "url": "/d/workflow-executions"
    },
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": [],
      "targetBlank": false,
      "title": "Tool Usage",
      "tooltip": "Detailed tool usage analytics",
      "type": "link",
